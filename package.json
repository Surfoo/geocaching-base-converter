{
    "name": "geocaching-base-converter",
    "version": "1.0.3",
    "keywords": [
        "geocaching",
        "geocode",
        "groundspeak",
        "geocache",
        "base31"
    ],
    "description": "Geocaching base converter",
    "license": "MIT",
    "author": "Surfoo",
    "repository": {
        "type": "git",
        "url": "https://github.com/surfoo/geocaching-base-converter.git"
    },
    "type": "module",
    "main": "index.js",
    "devDependencies": {
        "@biomejs/biome": "^2.3.6",
        "tap": "^21.1"
    },
    "scripts": {
        "test": "tap test/*.js",
        "test:coverage": "tap test/*.js --coverage",
        "lint": "npx @biomejs/biome lint .",
        "lint:fix": "npx @biomejs/biome lint --write .",
        "format": "npx @biomejs/biome format --write .",
        "check": "npx @biomejs/biome check --write .",
        "ci": "npm run check && npm test"
    }
}
