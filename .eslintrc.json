{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": ["standard", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error"
    }
}
